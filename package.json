{
  "name": "n8n-nodes-fusionauth",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for FusionAuth identity and access management providing 12 resources and 80+ operations for user management, applications, tenants, groups, registrations, identity providers, consents, forms, lambdas, webhooks, and audit logs.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-fusionauth.git"
  },
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-fusionauth",
  "bugs": {
    "url": "https://github.com/Velocity-BPA/n8n-nodes-fusionauth/issues"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "fusionauth",
    "identity",
    "authentication",
    "sso",
    "oauth",
    "oidc",
    "saml",
    "mfa",
    "user-management",
    "access-management"
  ],
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/FusionAuthApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/FusionAuth/FusionAuth.node.js",
      "dist/nodes/FusionAuth/FusionAuthTrigger.node.js"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-svg-sprite": "^2.0.3",
    "jest": "^29.7.0",
    "n8n-workflow": "^1.30.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "n8n-workflow": ">=1.0.0"
  }
}
